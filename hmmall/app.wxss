/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
view, text, image, navigator, button, scroll-view, swiper, swiper-item {
  box-sizing: border-box;
}
image {
  display: block;
}